<tr>
    <td> <%= user.id %> </td>
    <td> <%= link_to user.username, user %> </td>
    <td> <%= user.created_at.strftime("%m/%e/%Y") %> </td>
    <% if current_user.admin? && !current_user?(user) && !user.admin? %>
        <td><%= link_to 'Delete', user, method: :delete, data: { confirm: "Are you sure?" } %></td>
    <%end%>
</tr>